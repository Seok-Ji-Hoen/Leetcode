# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def middleNode(self, head: ListNode) -> ListNode:
        
        output = []
        
        while head:
            output.append(head)
            head = head.next
        
        return output[len(output)//2]     
